



<h1> Log In </h1>

<%= form_tag sessions_path do %>

<div class="field">
	<%= label_tag :email %>
	<%= text_field :email, params[:email] %>
</div>

<div class="field">
	<%= label_tag :password %>
	<%= text_field :password %>
</div>

<div class="action">
	<%= submit_tag "Log In" %>
</div>

<%end>